<html ng-app="wice">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">

	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="<%= appDescription %>" />
	<meta name="author" content="" />

	<title><%= appName %></title>

	<!--WALIRIANICE required -->
		<link rel="stylesheet" type="text/css" href="/toastr/toastr.min.css" />
		<!--JQueryUI-->
		<link rel="stylesheet" type="text/css" href="jquery-ui/themes/base/jquery-ui.min.css">
		<!-- dropzone -->
		<link rel="stylesheet" type="text/css" href="ng-dropzone/dist/ng-dropzone.min.css"/>
		<!-- select2 -->
		<link rel="stylesheet" type="text/css" href="select2/select2.css" />
		<!-- quill -->
		<link href="//cdn.quilljs.com/1.3.6/quill.core.css" rel="stylesheet">
        <link rel="stylesheet" href="//cdn.quilljs.com/1.3.6/quill.snow.css">
        <link rel="stylesheet" href="//cdn.quilljs.com/1.3.6/quill.bubble.css">
		<!-- angular-bootstrap -->
		<link rel="stylesheet" href="angular-bootstrap/ui-bootstrap-csp.css">
		<!-- dropzone -->
		<link rel="stylesheet" href="dropzone/dist/dropzone.css">



    <!--Loaded THEME CSSs file-->
    <%- include(stylesheets) %>


</head>

    <%- include(bodyTemplate) %>

	<!-- Bottom Scripts -->


	<!--WALIRIANICE required -->

	<!-- jquery -->
	<script src="jquery/dist/jquery.min.js"></script>
	<script src="jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="jquery-ui/jquery-ui.min.js"></script>
	<!-- popper -->
	<script src="popper.js/dist/umd/popper.min.js"></script>
	<script src="popper.js/dist/umd/popper-utils.min.js"></script>
	<!-- angular -->
	<script src="angular/angular.min.js"></script>
	<script src="angular-sanitize/angular-sanitize.min.js"></script>

	<!-- ui-router -->
	<script src="angular-ui-router/release/angular-ui-router.min.js"></script>

	<!-- bootstrap -->
	<script src="bootstrap/dist/js/bootstrap.js"></script>
	<script src="/toastr/toastr.min.js"></script>

	<!-- angular animate -->
	<script src="angular-animate/angular-animate.min.js"></script>

	<!-- angular ui bootstrap -->
	<script src="angular-bootstrap/ui-bootstrap-tpls.min.js"></script>

	<!-- objectId -->
	<script src="libs-core/objectId/Objectid.js"></script>

	<!-- lazyload -->
	<script src="oclazyload/dist/ocLazyLoad.min.js"></script>

	<!-- transalation ng-i18next -->
	<script src="i18next/i18next.js"></script>
	<script src="i18next-xhr-backend/i18nextXHRBackend.min.js"></script>
	<script src="i18next-sprintf-postprocessor/i18nextSprintfPostProcessor.min.js"></script>
	<script src="ng-i18next/dist/ng-i18next.min.js"></script>
	<script>
		if (window.i18next) {
			window.i18next
				.use(window.i18nextXHRBackend)
				.use(window.i18nextSprintfPostProcessor);

			window.i18next.use({
				name: 'patrick',
				type: 'postProcessor',
				process: function (value, key, options) {
					//https://www.youtube.com/watch?v=YSzOXtXm8p0
					return 'No, this is Patrick!';
				}
			});

			window.i18next.init({
				//lng: 'de', // If not given, i18n will detect the browser language.
				fallbackLng: 'en', // Default is dev
				saveMissing: true,
				saveMissingTo: 'all',
				backend: {
					loadPath: 'locales/{{lng}}/translation.json',
					addPath: '/api/v3/auth/missing-translation/{{lng}}'
				}/*,
				postProcess: 'patrick'*/
			});
		}
	</script>
	<!-- angular-loading-overlay -->
	<script src="angular-loading-overlay/dist/angular-loading-overlay.js"></script>
	<!-- angular-moment -->
	<script src="moment/moment.js"></script>
	<script src="angular-moment/angular-moment.min.js"></script>
	<!-- dropzone -->
	<script src="dropzone/dist/dropzone.js"></script>
	<script src="ng-dropzone/dist/ng-dropzone.min.js"></script>
	<!-- ng-file-upload -->
	<script src="ng-file-upload/ng-file-upload.min.js"></script>
	<!-- printThis -->
    <script src="printThis/printThis.js"></script>
	<!-- angular-select2 -->
    <script type="text/javascript" src="select2/select2.min.js"></script>
    <script src="angular-select2/dist/angular-select2.min.js"></script>
    <!-- angular-ui-select2 -->
	<script type="text/javascript" src="angular-ui-select2/src/select2.js"></script>
	<!-- angular-uuid -->
	<script type="text/javascript" src="angular.uuid2/dist/angular-uuid2.min.js"></script>
	<!-- ng-Quill -->
	<script type="text/javascript" src="//cdn.quilljs.com/1.3.6/quill.min.js"></script>
	<!-- bootstrap-datepicker -->
	<script type="text/javascript" src="bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
	<!-- angular-vs-repeat -->
	<script type="text/javascript" src="angular-vs-repeat/dist/angular-vs-repeat.min.js"></script>
	<!-- socket.io -->
	<script type="text/javascript" src="socket.io-client/dist/socket.io.js"></script>
	<script type="text/javascript" src="angular-socket-io/socket.min.js"></script>
	<!-- web notifications -->
	<script type="text/javascript" src="simple-web-notification/web-notification.js"></script>
	<script type="text/javascript" src="angular-web-notification/angular-web-notification.js"></script>
	<!-- easyPieChart -->
    <script src="jquery.easy-pie-chart/dist/angular.easypiechart.min.js"></script>
	<!-- drag&drop -->
    <script src="angular-draganddrop/angular-draganddrop.min.js"></script>
	<!-- Loading overlay  data-explorer-->
    <script src="angular-loading-overlay/dist/angular-loading-overlay.js"></script>
	<!-- tg-dynamic-directive data-explorer -->
    <script src="tg-dynamic-directive/src/tg.dynamic.directive.js"></script>
	<!-- angularjs-gauge -->
    <script src="angularjs-gauge/src/angularjs-gauge.js"></script>
	<!-- ui-sortable -->
	<script type="text/javascript" src="angular-ui-sortable/sortable.min.js"></script>
	<!-- dragula -->
	<script src="angularjs-dragula/dist/angularjs-dragula.min.js"></script>

<!--END WALIRIANICE required -->

	<!-- other -->
	<script type="text/javascript" src="angular-fullscreen/src/angular-fullscreen.js"></script>
	<script type="text/javascript" src="angular-cookies/angular-cookies.min.js"></script>

	<!-- angular app -->
	<script type="text/javascript" src="/core/webapp.js"></script>
	<script type="text/javascript" src="core/services.js"></script>

	<!--autoloaded scripts from theme-->
	<% scripts.forEach(function(script) { %>
	    <!--<%= script %>-->
        <script type="text/javascript" src="<%= script %>"></script>
    <% }); %>

    <!--Loaded THEME Scripts file -->
    <%- include(javascripts) %>

	<script>
	    $(document).ready(function(){
	        $('[data-toggle="tooltip"]').tooltip();
	    });
	</script>

</html>
